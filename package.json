{
  "name": "srn-toolkit",
  "version": "1.3.1",
  "description": "Toolkit for Development",
  "main": "./out/main/main.js",
  "author": "sunrisenew",
  "homepage": "https://sunrisenew.cn",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "npm run dev",
    "dev": "electron-vite dev -w",
    "preview": "electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "docs:build": "vuepress-vite build docs",
    "docs:clean-dev": "vuepress-vite dev docs --clean-cache",
    "docs:dev": "vuepress-vite dev docs",
    "docs:update-package": "npx vp-update"
  },
  "dependencies": {
    "@electron-toolkit/preload": "3.0.0",
    "@electron-toolkit/utils": "3.0.0",
    "cross-spawn": "7.0.3",
    "driver.js": "1.3.1",
    "electron-serve": "1.3.0",
    "electron-updater": "6.1.8",
    "globby": "14.0.1",
    "lodash-es": "^4.17.21",
    "naive-ui": "2.38.1",
    "pinia": "2.1.7",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@rushstack/eslint-patch": "1.7.2",
    "@sicons/ionicons5": "0.12.0",
    "@types/node": "20.11.24",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vuepress/bundler-vite": "2.0.0-rc.8",
    "electron": "28.2.5",
    "electron-builder": "24.12.0",
    "electron-vite": "2.0.0",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.22.0",
    "stylus": "0.62.0",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vue": "3.4.21",
    "vue-tsc": "1.8.27",
    "vuepress": "2.0.0-rc.8",
    "vuepress-theme-hope": "2.0.0-rc.27"
  }
}
